<mvc:View controllerName="fabio.palombi.MyProject.controller.Cart" displayBlock="true" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">

	<Page title="{i18n>cartTitle}" id="page2" titleAlignment="Center">

		
		<Table items="{cart>/}" headerText="test">
            <columns>
            <Column>
                <Text text="ID" />
            </Column>
            <Column>
                <Text text="Price" />
            </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <Text text="{cart>ID}" />
                    <Text />
                </ColumnListItem>
            </items>
        </Table>

	</Page>

</mvc:View>